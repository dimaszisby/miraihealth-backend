{
  "name": "miraihealth-backend",
  "version": "1.0.0",
  "main": "./src/server.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start": "node --loader ts-node/esm ./src/server.ts",
    "dev": "nodemon --exec tsx watch ./src/server.ts",
    "migrate": "sequelize db:migrate --config src/config/database.ts --env development",
    "migrate:dev": "ts-node --transpileOnly ./node_modules/.bin/sequelize db:migrate",
    "migrate:test": "NODE_ENV=test npx sequelize-cli db:migrate --config src/config/config.cjs",
    "migrate:undo": "sequelize db:migrate:undo --config src/config/database.ts --env development",
    "migrate:undo:all": "sequelize db:migrate:undo:all --config src/config/database.ts --env development",
    "migrate:test:undo": "NODE_ENV=test npx sequelize-cli db:migrate:undo --config src/config/config.cjs",
    "migrate:test:undo:all": "NODE_ENV=test npx sequelize-cli db:migrate:undo:all --config src/config/config.cjs",
    "test": "NODE_ENV=test NODE_OPTIONS='--experimental-vm-modules --es-module-specifier-resolution=node' jest --runInBand --detectOpenHandles"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.7.0",
    "sequelize": "^6.37.5",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.14",
    "@types/json-schema": "^7.0.15",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.13.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}